{
  "name": "puhelinluettelonbackend",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:ui": "npm run clean:win && npm run clean:lin && cd frontend && npm install && npm run build && npm run move:win && npm run move:win",
    "clean:win": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist build rmdir /Q /S build",
    "clean:lin": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf build",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon index.js",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "lint": "eslint .",
    "test:api": "jest --verbose --runInBand",
    "test:e2e": "cypress run ./cypress/e2e",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "puhelinluettelo"
  },
  "author": "Heikki Malkavaara",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "eslint": "^8.36.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "globalTeardown": "./tests/teardown.js" 
  }
}
